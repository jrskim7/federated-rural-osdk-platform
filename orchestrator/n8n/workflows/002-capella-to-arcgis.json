{
  "name": "002 - Capella â†’ ArcGIS: Trigger converter",
  "nodes": [
    {
      "parameters": {
        "path": "capella-run",
        "responseMode": "onReceived"
      },
      "id": "webhook-node",
      "name": "Capella Run Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "url": "http://mbse-bridge:5000/run/capella-to-arcgis",
        "method": "POST",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            { "name": "mbse_path", "value": "={{$json[\"mbse_path\"] || '/git_repo/mbse/exports/sample_capella_export.json'}}" },
            { "name": "out_path", "value": "='/git_repo/mbse/exports/output_geojson.json'" }
          ]
        }
      },
      "id": "call-bridge",
      "name": "Call MBSE Bridge",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [500, 300]
    }
  ],
  "connections": {
    "Capella Run Webhook": { "main": [ [ { "node": "Call MBSE Bridge", "type": "main", "index": 0 } ] ] }
  }
}